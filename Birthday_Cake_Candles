public static int birthDayCakeCandles(List<Integer> candles){
    int max=candles.get(0);
    int count=0;
    int l=candles.size();
    for(int i=1;i<l;i++){
      if(max<candles,get(i)){
        max=candles.get(i);
      }
    }
    for(int i=0;i<l;i++){
      if(max==candles.get(i)){
        count++;
      }
    }
    return count;
}//method
